{
  "name": "light-print",
  "version": "1.0.0",
  "description": "A lightweight print tool for the browser",
  "main": "dist/light-print.umd.js",
  "module": "dist/light-print.esm.js",
  "files": ["dist", "types/index.d.ts"],
  "types": "types/index.d.ts",
  "scripts": {
    "build": "npm run rollup && npm run build:types",
    "rollup": "rimraf dist && rollup -c scripts/rollup.config.js",
    "build:types": "rimraf types && tsc -p tsconfig.json --emitDeclarationOnly",
    "lint": "prettier --write --parser typescript \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xunmi1/light-print.git"
  },
  "sideEffects": false,
  "keywords": [
    "print",
    "browser print"
  ],
  "author": "xunmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xunmi1/light-print/issues"
  },
  "homepage": "https://github.com/xunmi1/light-print#readme",
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-sucrase": "^3.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.2",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  }
}
